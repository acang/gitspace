<class 
    name="com.web.obj.Ipuserinfo" 
    table="IPUSERINFO"
>
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="IPUSERINFO"
    </meta>

    <id
        name="id"
        type="java.lang.Long"
        column="ID"
    >
        <meta attribute="field-description">
           @hibernate.id
            generator-class="assigned"
            type="java.lang.Long"
            column="ID"


        </meta>
        <generator class="assigned" />
    </id>

    <property
        name="username"
        type="java.lang.String"
        column="USERNAME"
        length="50"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="USERNAME"
            length="50"
        </meta>    
    </property>
    <property
        name="ipaddress"
        type="java.lang.String"
        column="IPADDRESS"
        length="20"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="IPADDRESS"
            length="20"
        </meta>    
    </property>
    <property
        name="logintime"
        type="java.sql.Timestamp"
        column="LOGINTIME"
        length="7"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="LOGINTIME"
            length="7"
        </meta>    
    </property>

    <!-- Associations -->
  

</class>
</hibernate-mapping>
