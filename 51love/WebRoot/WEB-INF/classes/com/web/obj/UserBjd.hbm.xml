<class 
    name="com.web.obj.UserBjd" 
    table="USER_BJD"
>
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="USER_BJD"
    </meta>

    <id
        name="hyid"
        type="java.lang.Long"
        column="HYID"
    >
        <meta attribute="field-description">
           @hibernate.id
            generator-class="assigned"
            type="java.lang.Long"
            column="HYID"


        </meta>
        <generator class="assigned" />
    </id>

    <property
        name="bjdnumber"
        type="long"
        column="BJDNUMBER"
        not-null="true"
        length="10"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="BJDNUMBER"
            length="10"
            not-null="true"
        </meta>    
    </property>

